{
  "name": "@litemw/router",
  "version": "0.1.4",
  "author": "VladDotH",
  "license": "MIT",
  "description": "Type-safe middlewares for koa router",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/litemw/router.git"
  },
  "bugs": {
    "url": "https://github.com/litemw/router/issues"
  },
  "homepage": "https://github.com/litemw/router#readme",
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "postbuild": "rimraf tsconfig.types.tsbuildinfo",
    "lint": "bun run eslint lib/**/*.ts",
    "lint:fix": "bun run lint --fix"
  },
  "dependencies": {
    "koa": "2.15.3",
    "koa-router": "12.0.1",
    "lodash-es": "4.17.21"
  },
  "devDependencies": {
    "@types/bun": "1.2.1",
    "@types/koa": "2.15.0",
    "@types/koa-router": "7.4.8",
    "@types/lodash-es": "4.17.12",
    "@types/node": "20.17.16",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "eslint": "8.57.1",
    "eslint-config-prettier": "8.10.0",
    "eslint-plugin-prettier": "4.2.1",
    "prettier": "2.8.8",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.7.3",
    "supertest": "7.0.0",
    "@types/supertest": "6.0.2",
    "tsafe": "1.8.5"
  },
  "peerDependencies": {
    "koa": "^2",
    "koa-router": "^12"
  },
  "peerDependenciesMeta": {
    "koa": {
      "optional": false
    },
    "koa-router": {
      "optional": false
    }
  },
  "files": [
    "dist/*.js",
    "dist/*.d.ts"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "lib": "lib"
  }
}
